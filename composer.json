{
    "name": "spatie/laravel-sitemap",
    "description": "Create and generate sitemaps with ease",
    "keywords": [
        "spatie",
        "laravel-sitemap"
    ],
    "homepage": "https://github.com/spatie/laravel-sitemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.5.x-dev",
        "nesbot/carbon": "^1.21",
        "spatie/crawler": "^2.0.2",
        "spatie/phpunit-snapshot-assertions": "^1.0.0",
        "spatie/temporary-directory": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.x-dev",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Sitemap\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  },
   "extra": {
       "laravel": {
           "providers": [
               "Spatie\\Sitemap\\SitemapServiceProvider"
           ]
       }
   },
    "minimum-stability": "dev",
    "prefer-stable": true
}
